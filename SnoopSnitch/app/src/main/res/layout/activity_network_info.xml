                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed"
                    android:text="@string/network_info_mcc_value"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/networkInfoPreviousMNC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed"
                    android:text="@string/network_info_mnc_value"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/networkInfoPreviousLAC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed"
                    android:text="@string/network_info_lac_value"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="normal" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>